{
  "name": "lmg",
  "version": "0.1.0",
  "description": "london museum guide",
  "main": "server/server.js",
  "scripts": {
    "dev": "webpack-dev-server --content-base public/ --hot --inline",
    "start": "nodemon --ignore client/ --ignore public/ server/server & webpack --watch",
    "test-o": "tape -r babel-register 'test/**/*.test.js' | tap-spec",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "rosie-brigham",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "enzyme": "^2.3.0",
    "jsx-loader": "^0.13.2",
    "nodemon": "^1.10.2",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "immutable": "^3.8.1",
    "jsdom": "^9.8.3",
    "lodash.assign": "^4.2.0",
    "object.assign": "^4.0.4",
    "react-addons-pure-render-mixin": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-redux-form": "^1.0.11",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
